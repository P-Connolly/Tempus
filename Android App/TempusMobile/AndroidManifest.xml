<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tempusmobile"
    android:versionCode="1"
    android:versionName="1.0" >
<uses-permission android:name="android.permission.INTERNET"/>
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.tempusmobile.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
           <activity
            android:name="com.example.tempusmobile.Login"
            android:label="@string/login" >
        </activity>
         <activity
            android:name="com.example.tempusmobile.SelectDay"
            android:label="@string/selectday" >
        </activity>
          <activity
            
            android:name="com.example.tempusmobile.DayChosen"
            android:label="@string/daychosen" >
        </activity>
        <activity
            
            android:name="com.example.tempusmobile.EnterCourse"
            android:label="@string/entercourse" >
        </activity>
          <activity
            android:name="com.example.tempusmobile.EnterDept"
            android:label="@string/enterdept" >
        </activity>
    </application>

</manifest>